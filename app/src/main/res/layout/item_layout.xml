<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="215dp">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="10dp"
        android:background="@drawable/item_view">

        <TextView
            android:id="@+id/saved_check"
            android:layout_width="118dp"
            android:layout_height="29dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="64dp"
            android:layout_marginTop="147dp"
            android:layout_marginEnd="190dp"
            android:layout_marginBottom="24dp"
            android:backgroundTint="@null"
            android:hint="Сумма"
            android:textColor="#000000"
            android:textSize="23sp"
            android:textStyle="bold"
            tools:ignore="RtlCompat">

        </TextView>

        <TextView
            android:id="@+id/date"
            android:layout_width="168dp"
            android:layout_height="20dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="55dp"
            android:layout_marginTop="19dp"
            android:layout_marginEnd="149dp"
            android:layout_marginBottom="161dp"
            android:backgroundTint="@null"
            android:gravity="bottom"
            android:hint="Дата и время"
            android:textAlignment="center"
            android:textColor="#8C8C8C"
            android:textSize="15sp"
            tools:ignore="RtlCompat" />

        <TextView
            android:id="@+id/water"
            android:layout_width="wrap_content"
            android:layout_height="28dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="60dp"
            android:layout_marginTop="91dp"
            android:layout_marginEnd="190dp"
            android:layout_marginBottom="81dp"

            android:hint="Объём воды"
            android:textColor="#0094FF"
            android:textSize="23dp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/textView11"
            android:layout_width="wrap_content"
            android:layout_height="25dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="197dp"
            android:layout_marginTop="75dp"
            android:layout_marginEnd="40dp"
            android:layout_marginBottom="100dp"
            android:gravity="center"
            android:text="Тарификация"
            android:textAlignment="center"
            android:textColor="#0094FF"
            android:textSize="18dp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/sections"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="192dp"
            android:layout_marginTop="101dp"
            android:layout_marginEnd="33dp"
            android:layout_marginBottom="15dp"
            android:gravity="center"
            android:hint="sections"
            android:textAlignment="center"
            android:textColor="#8C8C8C"
            android:textSize="14dp" />

        <ImageView
            android:id="@+id/imageView27"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="13dp"
            android:layout_marginEnd="313dp"
            app:srcCompat="@drawable/date" />

        <ImageView
            android:id="@+id/imageView28"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="84dp"
            android:layout_marginEnd="313dp"
            app:srcCompat="@drawable/water_ic" />

        <ImageView
            android:id="@+id/imageView29"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="141dp"
            android:layout_marginEnd="313dp"
            app:srcCompat="@drawable/money" />

        <ImageView
            android:id="@+id/imageView23"
            android:layout_width="332dp"
            android:layout_height="2dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="20dp"
            android:layout_marginTop="48dp"
            android:layout_marginEnd="20dp"
            android:layout_marginBottom="148dp"
            app:srcCompat="@drawable/horizontal_line" />

        <ImageView
            android:id="@+id/imageView24"
            android:layout_width="2dp"
            android:layout_height="109dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="180dp"
            android:layout_marginTop="78dp"
            android:layout_marginEnd="180dp"
            android:layout_marginBottom="13dp"
            app:srcCompat="@drawable/vertical_line" />

        <ImageView
            android:id="@+id/imageView26"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginEnd="21dp"
            android:layout_marginBottom="155dp"
            app:srcCompat="@drawable/trash" />

    </RelativeLayout>
</FrameLayout>